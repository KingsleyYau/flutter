# CrashlyticsExample
use_frameworks!
platform :ios, '8.0'

target 'CrashlyticsExample'  do

  pod 'Firebase/Core'
  pod 'Fabric'
  pod 'Crashlytics'

  target 'CrashlyticsExampleTests' do
    inherit! :search_paths
  end
end

target 'CrashlyticsExampleSwift' do
  pod 'Firebase/Core'
  pod 'Fabric'
  pod 'Crashlytics'
end
