  pod 'Firebase'
  pod 'Firebase/InAppMessagingDisplay'
  pod 'Firebase/DynamicLinks'

  target 'InAppMessagingExampleSwiftTests' do
    inherit! :search_paths
  end

  target 'InAppMessagingExampleSwiftUITests' do
    inherit! :search_paths
  end
end
